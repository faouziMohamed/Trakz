{
  "name": "trakz",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "up": "yarn upgrade --latest",
    "start": "ng serve --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "eslint --max-warnings=0 ./**/*.{ts,html} --fix && yarn format:fix",
    "lint:strict": "eslint  --max-warnings=0 ./**/*.{ts,html}",
    "format:fix": "prettier -w ./**/*.{ts,html,json,scss,md,y*ml}",
    "format:check": "prettier -c ./**/*.{ts,html,json,scss,md,y*ml}",
    "type-check": "tsc --noEmit --incremental false",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "container:run": "docker compose -f docker-compose.yml up",
    "container:build": "docker build -t mfaouzi ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@cloudinary/ng": "^1.6.0",
    "@cloudinary/url-gen": "^1.8.6",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.8",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.8",
    "@angular/compiler-cli": "^14.2.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lit-a11y": "^1.1.0-next.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^45.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.2",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.18",
    "prettier": "^2.8.0",
    "prettier-eslint": "^15.0.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "app/src/**/*.{ts,html}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "./**/*.{ts,html,json,scss,md,y*ml}": [
      "prettier -w"
    ]
  }
}
